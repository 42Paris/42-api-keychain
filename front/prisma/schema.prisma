generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model keys {
  id                 Int      @id
  name               String
  client_id          String 
  client_secret      String
  secret_valid_until DateTime @db.Timestamptz(3)
  owned_by           String
  created_at         DateTime @default(now())
}

model settings {
  setting_key   String @unique
  setting_value String
}
